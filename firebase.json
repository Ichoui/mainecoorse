{
  "hosting": {
    "public": "dist/apps/front",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "dist/apps/back",
      "apps/back"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "functions": [
    {
      "source": "dist/apps/back",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "dist/apps/front",
        "apps/front"
      ]
    }
  ]
}
