  "scripts": {
    "start:front": "nx serve front --host --open",
    "start:back": "nx serve back",
    "preview:front": "nx preview front --host --open",
    "lint:back": "nx lint back",
    "lint:front": "nx lint front",
    "prettier": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md,json,scss} --config ./.prettierrc",
    "rename-env": "cp ./apps/back/.env.prod dist/apps/back/.env",
    "firebase:logs": "firebase functions:log",
    "firebase:serve-functions": "firebase serve --only functions",
    "firebase:emulator": "firebase emulators:start --only functions --inspect-functions --debug",
    "ncu": "ncu",
    "test:ncu": "ncu --doctor -u",
    "build:front": "nx build front --skip-nx-cache --emptyOutDir=true",
    "build:back": "nx build back --skip-nx-cache && npm run rename-env",
    "build:front:prod": "nx build front --production --skip-nx-cache --emptyOutDir=true",
    "build:back:prod": "nx build back --skip-nx-cache && npm run rename-env",
    "firebase:deploy:front": "firebase deploy --only hosting",
    "firebase:deploy:back": "firebase deploy --only functions --force"
  },
  "private": true,
